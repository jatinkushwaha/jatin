pipeline
{
 agent any
        stages {
             stage(" Git Checkout")  {
                     steps  {
                       git branch: 'master' , url: "https://github.com/jatinkushwaha/my-app.git"
                       
                       }
                  }
             stage("Maven-Build")   {
                    steps   {
                    sh  "mvn package"
              }
             }
                       
                    
                      
                     
          
